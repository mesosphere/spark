{
    "assets": {
        "container": {
            "docker": {
                "spark_docker": "{{docker-image}}"
            }
        }
    },
    "images": {
        "icon-medium": "https://downloads.mesosphere.io/spark/assets/icon-service-spark-medium.png",
        "icon-small": "https://downloads.mesosphere.io/spark/assets/icon-service-spark-small.png",
        "icon-large": "https://downloads.mesosphere.io/spark/assets/icon-service-spark-large.png"
    },
    "cli":{
        "binaries":{
            "darwin":{
                "x86-64":{
                    "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-spark-darwin}}" } ],
                    "kind":"executable",
                    "url":"{{artifact-dir}}/dcos-spark-darwin"
                }
            },
            "linux":{
                "x86-64":{
                    "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-spark-linux}}" } ],
                    "kind":"executable",
                    "url":"{{artifact-dir}}/dcos-spark-linux"
                }
            },
            "windows":{
                "x86-64":{
                    "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-spark.exe}}" } ],
                    "kind":"executable",
                    "url":"{{artifact-dir}}/dcos-spark.exe"
                }
            }
        }
    }
}
